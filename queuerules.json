{
  "rules": [
    {
      "then": {
        "queue": "General"
      }
    },
    {
      "when": {
        "tags": ["ANY", ["#news", "#politics", "#business"]]
      },
      "then": {
        "queue": "News Team"
      }
    },
    {
      "when": {
        "tags": ["ANY", ["#football", "#cricket", "#rugby"]]
      },
      "then": {
        "queue": "Sports Team"
      }
    }
  ]
}
